    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.userName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.phoneNum)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Magnitude)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Landitude)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.creditNum)
        </th>
        <th>
