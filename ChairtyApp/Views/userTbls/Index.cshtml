    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.userName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.phoneNum)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Magnitude)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Landitude)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.creditNum)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.creditType)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ruleTbl.ruleName)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.userId }) |
            @Html.ActionLink("Details", "Details", new { id=item.userId }) |
